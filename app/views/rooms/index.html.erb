<div class="col-md-4 col-md-offset-4">
    <table>
      <tr>
      <td>Theater</td>
      <td>Room</td>
      </tr>
    <%@rooms.each do |room| %>
        <tr>
        <td><%= room.theater.name %></td>
        <td>Columns:<%= room.template.col%> Rows:<%=room.template.row %> Total:<%=room.template.row * room.template.col%></td>
         <td><%=link_to "Edit", edit_room_path(room),class: "btn btn-info btn-sm" %></td>
         <td> <%=link_to "Destroy",room_path(room),method: :delete,class: "btn btn-info btn-sm"%> </td>
        </tr><br>
    <% end %>
    </table>

    <%=link_to "Create", new_room_url,class: "btn btn-info btn-sm" %>
</div>